{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "background": "aliceblue",
  "padding": 5,
  "width": 480,
  "title": {
    "text": "Visitor Arrivals (Jul 2018 - Jun 2025)",
    "fontSize": 14,
    "fontWeight": "bold",
    "anchor": "middle"
  },
  "data": [
    {"name": "brush_store"},
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/vanillaICY/3179_Assignment2/refs/heads/main/data/Visitor_Arrivals.csv",
      "format": {"type": "csv", "parse": {"Month": "date"}, "delimiter": ","}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "field": "Month",
          "type": "timeunit",
          "units": ["year", "month"],
          "as": ["yearmonth_Month", "yearmonth_Month_end"]
        },
        {
          "type": "impute",
          "field": "Arrivals_M",
          "groupby": [],
          "key": "yearmonth_Month",
          "method": "value",
          "value": 0
        },
        {
          "type": "stack",
          "groupby": ["yearmonth_Month"],
          "field": "Arrivals_M",
          "sort": {"field": [], "order": []},
          "as": ["Arrivals_M_start", "Arrivals_M_end"],
          "offset": "zero"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "impute",
          "field": "Arrivals_M",
          "groupby": [],
          "key": "Month",
          "method": "value",
          "value": 0
        },
        {
          "type": "stack",
          "groupby": ["Month"],
          "field": "Arrivals_M",
          "sort": {"field": [], "order": []},
          "as": ["Arrivals_M_start", "Arrivals_M_end"],
          "offset": "zero"
        }
      ]
    }
  ],
  "signals": [
    {"name": "concat_0_height", "value": 300},
    {"name": "concat_1_height", "value": 60},
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "pointermove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {
      "name": "brush",
      "update": "vlSelectionResolve(\"brush_store\", \"union\")"
    }
  ],
  "layout": {"padding": 20, "columns": 1, "bounds": "full", "align": "each"},
  "marks": [
    {
      "type": "group",
      "name": "concat_0_group",
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_0_height"}
        }
      },
      "marks": [
        {
          "name": "concat_0_marks",
          "type": "area",
          "clip": true,
          "style": ["area"],
          "sort": {"field": "x"},
          "interactive": false,
          "from": {"data": "data_0"},
          "encode": {
            "update": {
              "fill": {"value": "royalblue"},
              "fillOpacity": {"value": 0.9},
              "stroke": {"value": "#1e40af"},
              "strokeWidth": {"value": 1},
              "orient": {"value": "vertical"},
              "description": {
                "signal": "\"Month (year-month): \" + (timeFormat(datum[\"yearmonth_Month\"], \"%Y-%m\")) + \"; Arrivals(Million): \" + (format(datum[\"Arrivals_M\"], \"\"))"
              },
              "x": {"scale": "concat_0_x", "field": "yearmonth_Month"},
              "y": {"scale": "concat_0_y", "field": "Arrivals_M_end"},
              "y2": {"scale": "concat_0_y", "field": "Arrivals_M_start"},
              "defined": {
                "signal": "isValid(datum[\"yearmonth_Month\"]) && isFinite(+datum[\"yearmonth_Month\"]) && isValid(datum[\"Arrivals_M\"]) && isFinite(+datum[\"Arrivals_M\"])"
              }
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_0_x",
          "orient": "bottom",
          "gridScale": "concat_0_y",
          "grid": true,
          "tickCount": {"signal": "ceil(width/40)"},
          "tickMinStep": {
            "signal": "datetime(2001, 1, 1, 0, 0, 0, 0) - datetime(2001, 0, 1, 0, 0, 0, 0)"
          },
          "domain": false,
          "labels": false,
          "aria": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "gridScale": "concat_0_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "domain": false,
          "labels": false,
          "aria": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        },
        {
          "scale": "concat_0_x",
          "orient": "bottom",
          "grid": false,
          "title": "Month",
          "format": "%Y-%m",
          "labelFlush": true,
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(width/40)"},
          "tickMinStep": {
            "signal": "datetime(2001, 1, 1, 0, 0, 0, 0) - datetime(2001, 0, 1, 0, 0, 0, 0)"
          },
          "zindex": 0
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "grid": false,
          "title": "Arrivals(Million)",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "zindex": 0
        }
      ]
    },
    {
      "type": "group",
      "name": "concat_1_group",
      "title": {
        "text": "Use this line chart to filter out the data based on time",
        "fontWeight": "normal",
        "frame": "group"
      },
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_1_height"}
        }
      },
      "signals": [
        {
          "name": "brush_x",
          "value": [],
          "on": [
            {
              "events": {
                "source": "scope",
                "type": "pointerdown",
                "filter": [
                  "!event.item || event.item.mark.name !== \"brush_brush\""
                ]
              },
              "update": "[x(unit), x(unit)]"
            },
            {
              "events": {
                "source": "window",
                "type": "pointermove",
                "consume": true,
                "between": [
                  {
                    "source": "scope",
                    "type": "pointerdown",
                    "filter": [
                      "!event.item || event.item.mark.name !== \"brush_brush\""
                    ]
                  },
                  {"source": "window", "type": "pointerup"}
                ]
              },
              "update": "[brush_x[0], clamp(x(unit), 0, width)]"
            },
            {
              "events": {"signal": "brush_scale_trigger"},
              "update": "[scale(\"concat_1_x\", brush_Month[0]), scale(\"concat_1_x\", brush_Month[1])]"
            },
            {
              "events": [{"source": "view", "type": "dblclick"}],
              "update": "[0, 0]"
            },
            {
              "events": {"signal": "brush_translate_delta"},
              "update": "clampRange(panLinear(brush_translate_anchor.extent_x, brush_translate_delta.x / span(brush_translate_anchor.extent_x)), 0, width)"
            },
            {
              "events": {"signal": "brush_zoom_delta"},
              "update": "clampRange(zoomLinear(brush_x, brush_zoom_anchor.x, brush_zoom_delta), 0, width)"
            }
          ]
        },
        {
          "name": "brush_Month",
          "on": [
            {
              "events": {"signal": "brush_x"},
              "update": "brush_x[0] === brush_x[1] ? null : invert(\"concat_1_x\", brush_x)"
            }
          ]
        },
        {
          "name": "brush_scale_trigger",
          "value": {},
          "on": [
            {
              "events": [{"scale": "concat_1_x"}],
              "update": "(!isArray(brush_Month) || (+invert(\"concat_1_x\", brush_x)[0] === +brush_Month[0] && +invert(\"concat_1_x\", brush_x)[1] === +brush_Month[1])) ? brush_scale_trigger : {}"
            }
          ]
        },
        {
          "name": "brush_tuple",
          "on": [
            {
              "events": [{"signal": "brush_Month"}],
              "update": "brush_Month ? {unit: \"concat_1\", fields: brush_tuple_fields, values: [brush_Month]} : null"
            }
          ]
        },
        {
          "name": "brush_tuple_fields",
          "value": [{"field": "Month", "channel": "x", "type": "R"}]
        },
        {
          "name": "brush_translate_anchor",
          "value": {},
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "pointerdown",
                  "markname": "brush_brush"
                }
              ],
              "update": "{x: x(unit), y: y(unit), extent_x: slice(brush_x)}"
            }
          ]
        },
        {
          "name": "brush_translate_delta",
          "value": {},
          "on": [
            {
              "events": [
                {
                  "source": "window",
                  "type": "pointermove",
                  "consume": true,
                  "between": [
                    {
                      "source": "scope",
                      "type": "pointerdown",
                      "markname": "brush_brush"
                    },
                    {"source": "window", "type": "pointerup"}
                  ]
                }
              ],
              "update": "{x: brush_translate_anchor.x - x(unit), y: brush_translate_anchor.y - y(unit)}"
            }
          ]
        },
        {
          "name": "brush_zoom_anchor",
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "wheel",
                  "consume": true,
                  "markname": "brush_brush"
                }
              ],
              "update": "{x: x(unit), y: y(unit)}"
            }
          ]
        },
        {
          "name": "brush_zoom_delta",
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "wheel",
                  "consume": true,
                  "markname": "brush_brush"
                }
              ],
              "force": true,
              "update": "pow(1.001, event.deltaY * pow(16, event.deltaMode))"
            }
          ]
        },
        {
          "name": "brush_modify",
          "on": [
            {
              "events": {"signal": "brush_tuple"},
              "update": "modify(\"brush_store\", brush_tuple, true)"
            }
          ]
        }
      ],
      "marks": [
        {
          "name": "brush_brush_bg",
          "type": "rect",
          "clip": true,
          "encode": {
            "enter": {
              "fill": {"value": "#333"},
              "fillOpacity": {"value": 0.125}
            },
            "update": {
              "x": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "signal": "brush_x[0]"
                },
                {"value": 0}
              ],
              "y": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "value": 0
                },
                {"value": 0}
              ],
              "x2": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "signal": "brush_x[1]"
                },
                {"value": 0}
              ],
              "y2": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "field": {"group": "height"}
                },
                {"value": 0}
              ]
            }
          }
        },
        {
          "name": "concat_1_marks",
          "type": "area",
          "style": ["area"],
          "sort": {"field": "x"},
          "interactive": true,
          "from": {"data": "data_1"},
          "encode": {
            "update": {
              "fill": {"value": "royalblue"},
              "fillOpacity": {"value": 0.9},
              "stroke": {"value": "#1e40af"},
              "strokeWidth": {"value": 1},
              "orient": {"value": "vertical"},
              "description": {
                "signal": "\"Month: \" + (timeFormat(datum[\"Month\"], \"%b %d, %Y\")) + \"; Arrivals: \" + (format(datum[\"Arrivals_M\"], \"\"))"
              },
              "x": {"scale": "concat_1_x", "field": "Month"},
              "y": {"scale": "concat_1_y", "field": "Arrivals_M_end"},
              "y2": {"scale": "concat_1_y", "field": "Arrivals_M_start"},
              "defined": {
                "signal": "isValid(datum[\"Month\"]) && isFinite(+datum[\"Month\"]) && isValid(datum[\"Arrivals_M\"]) && isFinite(+datum[\"Arrivals_M\"])"
              }
            }
          }
        },
        {
          "name": "brush_brush",
          "type": "rect",
          "clip": true,
          "encode": {
            "enter": {
              "cursor": {"value": "move"},
              "fill": {"value": "transparent"}
            },
            "update": {
              "x": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "signal": "brush_x[0]"
                },
                {"value": 0}
              ],
              "y": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "value": 0
                },
                {"value": 0}
              ],
              "x2": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "signal": "brush_x[1]"
                },
                {"value": 0}
              ],
              "y2": [
                {
                  "test": "data(\"brush_store\").length && data(\"brush_store\")[0].unit === \"concat_1\"",
                  "field": {"group": "height"}
                },
                {"value": 0}
              ],
              "stroke": [
                {"test": "brush_x[0] !== brush_x[1]", "value": "white"},
                {"value": null}
              ]
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_1_x",
          "orient": "bottom",
          "gridScale": "concat_1_y",
          "grid": true,
          "tickCount": {"signal": "ceil(width/40)"},
          "domain": false,
          "labels": false,
          "aria": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        },
        {
          "scale": "concat_1_x",
          "orient": "bottom",
          "grid": false,
          "title": "Month",
          "labelFlush": true,
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(width/40)"},
          "zindex": 0
        },
        {
          "scale": "concat_1_y",
          "orient": "left",
          "grid": false,
          "title": "Arrivals",
          "tickCount": 3,
          "labelOverlap": true,
          "zindex": 0
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "concat_0_x",
      "type": "time",
      "domain": {"data": "data_0", "field": "yearmonth_Month"},
      "domainRaw": {"signal": "brush[\"Month\"]"},
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "concat_0_y",
      "type": "linear",
      "domain": {
        "data": "data_0",
        "fields": ["Arrivals_M_start", "Arrivals_M_end"]
      },
      "range": [{"signal": "concat_0_height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "concat_1_x",
      "type": "time",
      "domain": {"data": "data_1", "field": "Month"},
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "concat_1_y",
      "type": "linear",
      "domain": {
        "data": "data_1",
        "fields": ["Arrivals_M_start", "Arrivals_M_end"]
      },
      "range": [{"signal": "concat_1_height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "config": {}
}